#!/bin/bash -e
. /opt/pg/env/alps.bashrc
cd ~/work/pg_ui/build
make -j 4 install
cd ~/work/sal/build
make -j 4 install
cd ~/work/pkg/build
make -j 4 install
cd ~/work/pg_cli/build
make -j 4 install
cd ~/work/python-plumgridlib/build
make -j 4 install
cd ~/work/python-plumgridlib/lib
sudo python setup.py install
cd ~/work/corelib/build
make -j 4 install
cd ~/work/alps/build
return_value=0
for i in `seq 1 4`; do
  make -k -j 4 install || return_value=$?
  [[ $return_value -eq 0 ]] && break
done
#Need to have the last command status for run_bg_cmd
[[ $return_value -eq 0  ]]
